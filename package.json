{
  "name": "eduninja-web",
  "version": "1.0.0",
  "description": "EduNinja Web端 - AI驱动的教育学习助手",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .vue,.js,.ts --fix",
    "format": "prettier --write src/**/*.{vue,js,ts,css,scss}",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
