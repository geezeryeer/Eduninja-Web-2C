    "element-plus": "^2.4.2",
    "highlight.js": "^11.9.0",
    "lodash-es": "^4.17.21",
    "markdown-it": "^13.0.2",
    "nprogress": "^0.2.0",
    "pdfjs-dist": "^2.16.105",
    "pinia": "^2.1.7",
    "sortablejs": "^1.15.0",
    "tailwindcss": "^3.3.5",
    "vue": "^3.3.8",
    "vue-chartjs": "^5.2.0",
    "vue-lazyload": "^3.0.0",
    "vue-router": "^4.2.5",
    "vue-virtual-scroller": "^2.0.0-beta.8",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.18.1",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.0.2",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^0.17.0",
    "vue-tsc": "^1.8.22",
    "workbox-window": "^7.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{vue,js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,less}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "vue",
    "education",
    "ai",
    "learning",
    "assistant",
    "web-app",
    "typescript"
  ],
  "author": "EduNinja Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/eduninja/web-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/eduninja/web-frontend/issues"
  },
  "homepage": "https://eduninja.com"
